      </Button>
      <Button fx:id="buyEsButton" mnemonicParsing="false" onAction="#OnActionBuyElectricStation" text="Buy Electric Station">
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="buyUniButton" mnemonicParsing="false" onAction="#OnActionBuyUni" text="Buy University">
