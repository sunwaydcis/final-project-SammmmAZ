            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="buyCommercialCenterButton" mnemonicParsing="false" onAction="#OnActionBuyCommercialCenter" text="Buy Commercial Center">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="quitButton" mnemonicParsing="false" onAction="#OnActionQuitButton" prefHeight="25.0" prefWidth="101.0" text="Quit Game">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin></Button>
   </children>
