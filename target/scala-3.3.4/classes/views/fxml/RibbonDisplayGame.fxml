<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="60.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BiomeMapApp.controller.RibbonBarGameController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
       <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" prefWidth="1280.0" spacing="10">
         <children>
            <Button fx:id="buttonToMap" mnemonicParsing="false" onAction="#OnActionToMaps" prefHeight="27.0" prefWidth="120.0" text="Map">
               <font>
                  <Font name="Cambria Bold Italic" size="14.0" />
               </font>
            </Button>
            <Button fx:id="buttonToStats" maxWidth="-Infinity" mnemonicParsing="false" onAction="#OnActionToStats" prefWidth="120.0" text="Statistics">
               <font>
                  <Font name="Cambria Bold Italic" size="14.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefWidth="120.0" text="Speed">
               <font>
                  <Font name="Cambria Bold Italic" size="14.0" />
               </font>
            </Button>
            <Label prefHeight="27.0" prefWidth="27.0" text="x1" textAlignment="CENTER">
               <font>
                  <Font name="Cambria Bold Italic" size="12.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="120.0" text="Pause">
               <font>
                  <Font name="Cambria Bold Italic" size="14.0" />
               </font>
            </Button>
         </children></HBox>
      <Separator prefHeight="13.0" prefWidth="600.0" />
   </children>
</VBox>
