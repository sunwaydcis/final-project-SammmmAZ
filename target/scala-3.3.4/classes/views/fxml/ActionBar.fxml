<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="actionBarVBox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="145.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BiomeMapApp.controller.ActionBarController">
   <children>
      <Label prefHeight="17.0" prefWidth="91.0" text="Game Progress">
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin>
      </Label>
      <ProgressBar fx:id="progressBar" prefHeight="25.0" prefWidth="145.0" progress="0" />
      <Button fx:id="addCityButton" mnemonicParsing="false" onAction="#OnActionAddCity" prefHeight="25.0" prefWidth="102.0" text="Add City">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin></Button>
      <Button fx:id="buyFarmButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="119.0" text="Buy Farm" onAction="#OnActionAddFarm">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin></Button>
      <Button fx:id="buyHospitalButton" mnemonicParsing="false" text="Buy Hospital" onAction="#OnActionAddHospital">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="buyCommercialCenter" mnemonicParsing="false" text="Buy Commercial Center" onAction="#OnActionBuyCommercialCenter">
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="quitButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="101.0" text="Quit Game" onAction="#OnActionQuitButton">
         <VBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </VBox.margin></Button>
   </children>
</VBox>
